{
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Electron Main",
            "runtimeExecutable": "${workspaceFolder}/MarkMath/node_modules/.bin/electron",
            "program": "${workspaceFolder}/MarkMath/main.js"
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/DotNetCore/bin/Debug/netcoreapp2.2/DotNetCore.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            // "program": "${workspaceFolder}/Python/LanguageInteroperability/Introp_CSharp.py",
            // "program": "${workspaceFolder}/Python/Turtle/Draw_Axis.py",
            // "program": "${workspaceFolder}/Python/Crawlers/NovelCrawler.py",
            "program": "${workspaceFolder}/Python/Practice/Day-01.py",
            // "program": "${workspaceFolder}/Python/API/BaiduTranslate.py",
            // Python\RestFulAPI\BaiduTranslate.py
            "console": "integratedTerminal"
        },
        {
            "name": "(Windows) Launch",
            "preLaunchTask": "build",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/cygwin64/bin",
            "setupCommands": [
                {
                    "description":"Enable pretty-printing for gbd",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}